{
  "name": "tidal-bulk-client",
  "version": "1.0.0",
  "description": "TypeScript client for Tidal API bulk operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "start": "ts-node src/index.ts",
    "dev": "ts-node --watch src/index.ts",
    "demo:server-backup": "ts-node examples/server-backup-demo.ts",
    "demo:hostname-fqdn": "ts-node examples/hostname-to-fqdn-demo.ts",
    "demo:cpu-utilization": "ts-node examples/cpu-utilization-update-demo.ts",
    "demo:cpu-utilization:dry-run": "ts-node examples/cpu-utilization-update-demo.ts --dry-run"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "tidal",
    "api",
    "bulk",
    "operations",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
